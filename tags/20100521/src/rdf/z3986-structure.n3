@base  <http://www.daisy.org/z3986/2011/vocab/structure/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix zt:  <http://www.daisy.org/ns/rdf/property#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix jena: <http://www.daisy.org/ns/rdf/jena#> .
@prefix foaf: <http://xmlns.com/foaf/spec/#term_> .
@prefix : <#> .

<>
  dcterms:title "Z39.86-2011 Structural Semantics Vocabulary" ;
  dcterms:description "This RDF vocabulary, a part of [[http://www.daisy.org/z3986/2011/Z3986-2011A.html ANSI/NISO Z39.86-2011 Part A]], defines a set of terms relating to the description of structural semantics of documents in general, and written works in particular." ;
  dcterms:publisher "DAISY Consortium" ;
  dcterms:date "2009-12-01" ;
  dcterms:creator "mgylling" ;
  dcterms:creator "mgarrish" ;
  dcterms:creator "mdemeglio" .


########################################
########## document structure #########
########################################

:document-structure	a rdf:Bag ;
					jena:bag-sort "10" ;
					dcterms:title "Major document structures" ;
					dcterms:description "Standard terms used for the classification, partitioning and dividing of a document." .



########## types #########

:document-types	a rdf:Bag ;
					jena:bag-sort "20" ;
					rdf:member :document-structure ;
					dcterms:title "Document types" .

:fiction			a rdf:Property ;
					rdfs:label "fiction" ;
					rdf:member :document-types ;
					rdfs:comment "A work of fiction." .

:non-fiction		a rdf:Property ;
					rdfs:label "non-fiction" ;
					rdf:member :document-types ;
					rdfs:comment "A work of non-fiction." .

:article			a rdf:Property ;
					rdfs:label "article" ;
					rdf:member :document-types ;
					rdfs:comment "A major independent section in a piece of writing, typically appearing in journals, magazines or newspapers." .

:essay				a rdf:Property ;
					rdfs:label "essay" ;
					rdf:member :document-types ;
					rdfs:comment "An essay." .

:textbook		a rdf:Property ;
					rdfs:label "textbook" ;
					rdf:member :document-types ;
					rdfs:comment "A textbook." .

:catalogue		a rdf:Property ;
					rdfs:label "catalogue" ;
					rdf:member :document-types ;
					rdfs:comment "A catalogue." .




########## partitions #########

:document-partitions	a rdf:Bag ;
					jena:bag-sort "30" ;
					rdf:member :document-structure ;
					dcterms:title "Document partitions" .

:frontmatter		a rdf:Property ;
					rdfs:label "frontmatter";
					rdf:member :document-partitions ;
					rdfs:comment "Preliminary material to the content, such as tables of contents, dedications, etc." .

:bodymatter		a rdf:Property ;
					rdfs:label "bodymatter";
					rdf:member :document-partitions ;
					rdfs:comment "The body content of a document." .

:backmatter		a rdf:Property ;
					rdfs:label "backmatter";
					rdf:member :document-partitions ;
					rdfs:comment "Ancillary material occurring after the document body, such as indices, appendices, etc.." .


########## divisions #########

:document-divisions	a rdf:Bag ;
					jena:bag-sort "30" ;
					rdf:member :document-structure ;
					dcterms:title "Document divisions" .


:volume		a rdf:Property ;
					rdfs:label "volume";
					rdf:member :document-divisions ;
					rdfs:comment "A component of a collection." .

:part			a rdf:Property;
					rdfs:label "part";
					rdf:member :document-divisions ;
					rdfs:comment "A major structural division of a piece of writing, typically encapsulating a set of related chapters." .

:chapter		a rdf:Property ;
					rdfs:label "chapter";
					rdf:member :document-divisions ;
					rdfs:comment "A major structural division of a piece of writing." .

:subchapter		a rdf:Property ;
					rdfs:label "subchapter";
					rdf:member :document-divisions ;
					rdfs:comment "A major sub-division of a chapter." .

:division		a rdf:Property ;
					rdfs:label "division";
					rdf:member :document-divisions ;
					rdfs:comment "A major structural division that may also appear as a substructure of a part (esp. in legislation)." .

:section		a rdf:Property ;
					rdfs:label "section";
					rdf:member :document-divisions ;
					rdfs:comment "A structural division typically subordinate in importance to a part or division (esp. in textbooks and legislation)." .

:subsection		a rdf:Property ;
					rdfs:label "subsection";
					rdf:member :document-divisions ;
					rdfs:comment "A major sub-division of a section." .





########## sections #########

:document-sections	a rdf:Bag ;
					jena:bag-sort "40" ;
					rdf:member :document-structure ;
					dcterms:title "Document sections" .

:foreword		a rdf:Property ;
					rdfs:label "foreword";
					rdf:member :document-sections ;
					rdfs:comment "An introductory section that precedes the work, typically not written by the work's author." .

:preface		a rdf:Property ;
					rdfs:label "preface";
					rdf:member :document-sections ;
					rdfs:comment "An introductory section that precedes the work, typically written by the work's author." .

:prologue		a rdf:Property ;
					rdfs:label "prologue" ;
					rdf:member :document-sections ;
					rdfs:comment "An introductory section that sets the background to a story, typically part of the narrative." .

:introduction		a rdf:Property ;
					rdfs:label "introduction";
					rdf:member :document-sections ;
					rdfs:comment "A section in the beginning of the work, typically introducing the reader to the scope or nature of the work's content".

:preamble		a rdf:Property ;
					rdfs:label "preamble" ;
					rdf:member :document-sections ;
					rdfs:comment "A section in the beginning of the work, typically containing introductory and/or explanatory prose regarding the scope or nature of the work's content".

:conclusion		a rdf:Property ;
					rdfs:label "conclusion" ;
					rdf:member :document-sections ;
					rdfs:comment "An ending section that typically wraps up the work." .

:epilogue		a rdf:Property ;
					rdfs:label "epilogue" ;
					rdf:member :document-sections ;
					rdfs:comment "A concluding section that is typically written from a later point in time than the main story, although still part of the narrative." .

:afterword		a rdf:Property ;
					rdfs:label "afterword" ;
					rdf:member :document-sections ;
					rdfs:comment "An ending section that typically details the history of the story or its significance." .





########## reference sections #########


:document-reference	a rdf:Bag ;
					jena:bag-sort "50" ;
					rdf:member :document-structure ;
					dcterms:title "Document reference sections" .

:toc			a rdf:Property ;
					rdfs:label "toc" ;
					rdf:member :document-reference ;
					rdfs:comment "A table of contents, typically appearing in the work's frontmatter, or at the beginning of a section." .

:appendix		a rdf:Property ;
					rdfs:label "appendix" ;
					rdf:member :document-reference ;
					rdfs:comment "Supplemental information." .

:glossary		a rdf:Property ;
					rdfs:label "glossary" ;
					rdf:member :document-reference ;
					rdfs:comment "An alphabetical list of terms in a particular domain of knowledge with the definitions for those terms." .

:bibliography		a rdf:Property ;
					rdfs:label "bibliography" ;
					rdf:member :document-reference ;
					rdfs:comment "A list of works cited." .

:discography		a rdf:Property ;
					rdfs:label "discography" ;
					rdf:member :document-reference ;
					rdfs:comment "A list of albums and/or songs by a performer." .

:filmography		a rdf:Property ;
					rdfs:label "filmography" ;
					rdf:member :document-reference ;
					rdfs:comment "A list of acting roles by a performer." .

:index			a rdf:Property ;
					rdfs:label "index" ;
					rdf:member :document-reference ;
					rdfs:comment "A detailed list, usually arranged alphabetically, of the specific information in a publication." .

:colophon		a rdf:Property ;
					rdfs:label "colophon" ;
					rdf:member :document-reference ;
					rdfs:comment "A brief description usually located at the end of a publication, describing production notes relevant to the edition." .


############### titles ##############

:titles			a rdf:Bag ;
					jena:bag-sort "60" ;
					rdf:member :document-structure ;
					dcterms:title "Titles" .

:title			a rdf:Property ;
					rdfs:label "title" ;
					rdf:member :titles ;
					rdfs:comment "The title of the work, or, when used in the context of a [[#fulltitle]], the primary part of the full title." .

:halftitle		a rdf:Property ;
					rdfs:label "halftitle" ;
					rdfs:subPropertyOf :title ;
					rdf:member :titles ;
					rdfs:comment "The title appearing that appears on the first page of a work or immediately before the text." .

:fulltitle			a rdf:Property ;
					rdfs:label "fulltitle" ;
					rdfs:subPropertyOf :title ;
					rdf:member :titles ;
					owl:sameAs dcterms:title;
					rdfs:comment "The full title of the work, either simple, in which case it is identical to [[#title]], or compound, in which case it consists of a [[#title]] and a [[#subtitle]]." .

:subtitle		a rdf:Property;
					rdfs:label "subtitle" ;
					rdfs:subPropertyOf :title ;
					rdf:member :titles ;
					rdfs:comment "An explanatory or alternate title for the work." .

:covertitle		a rdf:Property;
					rdfs:label "covertitle" ;
					rdfs:subPropertyOf :title ;
					rdf:member :titles ;
					rdfs:comment "The title of the work as displayed on the work's cover." .



########## preliminary materials #########

:document-preliminaries	a rdf:Bag ;
					jena:bag-sort "70" ;
					rdf:member :document-structure ;
					dcterms:title "Preliminary sections and components" .

:titlepage		a rdf:Property ;
					rdfs:label "titlepage" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "The title page of the work." .

:halftitlepage		a rdf:Property ;
					rdfs:label "halftitlepage" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "The half title page of the work." .

:acknowledgments	a rdf:Property ;
					rdfs:label "acknowledgments" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A passage containing acknowledgments to entities involved in the realization of the work." .

:imprint		a rdf:Property ;
					rdfs:label "imprint" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "Information relating to the publication or distribution of the work." .

:imprimatur		a rdf:Property ;
					rdfs:label "imprimatur" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A formal statement authorizing the publication of the work." .

:illustrations-list	a rdf:Property ;
					rdfs:label "illustrations-list" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A listing if of illustrations included in the work." .

:tables-list		a rdf:Property ;
					rdfs:label "tables-list" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A listing of tables included in the work." .

:publisher-address	a rdf:Property ;
					rdfs:label "publisher-address" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "The publisher's address, which can be abbreviated to city and postal code only." .

:publisher-note		a rdf:Property ;
					rdfs:label "publisher-address" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A note from the publisher." .

:editorial-note		a rdf:Property ;
					rdfs:label "editorial-note" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "An explanation of an editor’s method, or a discussion of variant texts." .

:grant-acknowledgment	a rdf:Property ;
					rdfs:label "grant-acknowledgment" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "Describes financial assistance towards the work." .

:contributors		a rdf:Property ;
					rdfs:label "contributors" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A list of contributors to the work." .

:other-credits		a rdf:Property ;
					rdfs:label "other-credits" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "Acknowledgments of previously published parts of the work, illustration credits, and permission to quote from copyrighted material." .

:biographical-note	a rdf:Property ;
					rdfs:label "biographical-note" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A biographical note about the author or authors, here including editors, compilers, and translators)." .

:translator-note	a rdf:Property ;
					rdfs:label "translator-note" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "An introductory note from the translator(s)." .

:errata			a rdf:Property ;
					rdfs:label "errata" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "Publication errata, in printed works typically a loose sheet inserted by hand; sometimes a bound page." .

:promotional		a rdf:Property ;
					rdfs:label "promotional-copy" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "A promotional description of the book, often containing quotations from reviews." .

:dedication		a rdf:Property ;
					rdfs:label "dedication" ;
					rdf:member :document-preliminaries ;
					rdfs:comment "An inscription addressed to one or several particular person(s)." .












########################################
########## document components #########
########################################

:document-components	a rdf:Bag ;
					jena:bag-sort "30" ;
					dcterms:title "Document components" ;
					dcterms:description "Non-structural components that occur within documents, such as letters, poems, quotations, etc." .




############# annotations ##############

:annotations		a rdf:Bag ;
					jena:bag-sort "35" ;
					rdf:member :document-components ;
					dcterms:title "Annotations" .

:annotation		a rdf:Property ;
					rdfs:label "annotation" ;
					rdf:member :annotations ;
					rdfs:comment "Explanatory information about passages in the work." .

:author-anno		a rdf:Property ;
					rdfs:label "author-anno" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation contributed by an author of the work." .

:editor-anno		a rdf:Property ;
					rdfs:label "editor-anno" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation contributed by an editor of the work." .

:commentator-anno	a rdf:Property ;
					rdfs:label "commentator-anno" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation contributed by a commentator of the work." .

:translator-anno	a rdf:Property ;
					rdfs:label "translator-anno" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation contributed by a translator of the work." .

:republisher-anno	a rdf:Property ;
					rdfs:label "republisher-anno" ;
					rdfs:subPropertyOf :annotation, :custom ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation contributed by a republisher." .

:structure-description	a rdf:Property ;
					rdfs:label "structure-description" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation whose purpose is to provide a description of a structure (e.g., a table structure summary)." .

:content-description	a rdf:Property ;
					rdfs:label "content-description" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation whose purpose is to provide a description of content  (e.g., an image description)." .

:introductory-note	a rdf:Property ;
					rdfs:label "introductory-note" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation whose purpose is to provide an introductory note." .

:commentary		a rdf:Property ;
					rdfs:label "commentary" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation whose purpose is to provide commentary." .

:clarification		a rdf:Property ;
					rdfs:label "clarification" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation whose purpose is to provide a clarification." .

:correction		a rdf:Property ;
					rdfs:label "correction" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation whose purpose is to provide a correction." .

:attribution		a rdf:Property ;
					rdfs:label "attribution" ;
					rdfs:subPropertyOf :annotation ;
					rdf:member :annotations ;
					zt:for "z3986.annotation" ;
					rdfs:comment "An annotation whose purpose is to provide a source or attribution." .



############ asides ##########

:asides			a rdf:Bag ;
					jena:bag-sort "40" ;
					rdf:member :document-components ;
					dcterms:title "Asides" .

:aside			a rdf:Property ;
					rdfs:label "aside" ;
					rdf:member :asides ;
					rdfs:comment "Secondary or supplementary content." .

:sidebar		a rdf:Property ;
					rdfs:label "sidebar" ;
					rdfs:subPropertyOf :aside ;
					rdf:member :asides ;
					rdfs:comment "Secondary or supplementary content, typically formatted as an inset or box." .

:practice		a rdf:Property ;
					rdfs:label "practice" ;
					rdfs:subPropertyOf :aside ;
					rdf:member :asides ;
					rdfs:comment "A review exercise or sample." .

:notice			a rdf:Property ;
					rdfs:label "notice" ;
					rdfs:subPropertyOf :aside ;
					rdf:member :asides ;
					rdfs:comment "A notification." .

:warning		a rdf:Property ;
					rdfs:label "warning" ;
					rdfs:subPropertyOf :aside ;
					rdf:member :asides ;
					rdfs:comment "A warning." .

:marginalia		a rdf:Property ;
					rdfs:label "marginalia" ;
					rdfs:subPropertyOf :aside ;
					rdf:member :asides ;
					rdfs:comment "Marginalia." .

:help			a rdf:Property ;
					rdfs:label "help" ;
					rdfs:subPropertyOf :aside ;
					rdf:member :asides ;
					rdfs:comment "Instructions/advice to the reader on using the work." .



############## dialogue/drama ##########

:dialogue-drama	a rdf:Bag ;
					jena:bag-sort "45" ;
					rdf:member :document-components ;
					dcterms:title "Dialogue/Drama" .

:drama			a rdf:Property ;
					rdf:member :dialogue-drama ;
					rdfs:label "drama" ;
					rdfs:comment "A work of fiction intended for performance." .

:scene			a rdf:Property ;
					rdf:member :dialogue-drama ;
					rdfs:label "scene" ;
					rdfs:comment "A structurally significant segment of a drama." .

:stage-direction	a rdf:Property ;
					rdfs:label "stage-direction" ;
					rdf:member :dialogue-drama ;
					rdfs:comment "An instruction to an actor or director, included in the work." .

:dramatis-personae	a rdf:Property ;
					rdf:member :dialogue-drama ;
					rdfs:label "dramatis-personae" ;
					rdfs:subPropertyOf :collection ;
					rdfs:seeAlso :persona ;
					rdfs:comment "A listing of the roles or characters appearing in the work." .

:persona		a rdf:Property ;
					rdf:member :dialogue-drama ;
					rdfs:label "persona" ;
					rdfs:seeAlso :dramatis-personae ;
					rdfs:comment "The name or identity of an individual role or character appearing in the work." .

:actor			a rdf:Property ;
					rdf:member :dialogue-drama ;
					rdfs:label "actor" ;
					rdfs:seeAlso :dramatis-personae ;
					rdfs:comment "The name of the actor who plays a character in the work." .

:role-description	a rdf:Property ;
					rdf:member :dialogue-drama ;
					rdfs:label "role-description" ;
					rdfs:seeAlso :dramatis-personae ;
					rdfs:comment "A description of a character in the work." .

:speech		a rdf:Property ;
					rdf:member :dialogue-drama ;
					rdfs:label "speech" ;
					rdfs:seeAlso :persona ;
					rdfs:comment "An utterance by a [[#persona]]." .



############ diaries ##########


:diaries			a rdf:Bag ;
					jena:bag-sort "50" ;
					rdf:member :document-components ;
					dcterms:title "Diaries" .

:diary			a rdf:Property ;
					rdfs:label "diary" ;
					rdf:member :diaries ;
					rdfs:comment "A diary." .

:diary-entry		a rdf:Property ;
					rdfs:label "diary-entry" ;
					rdf:member :diaries ;
					rdfs:comment "A diary entry." .



########### epigraphs ###############

:epigraph		a rdf:Property ;
					rdfs:label "epigraph" ;
					rdf:member :document-components ;
					rdfs:comment "A quotation that is pertinent but not integral to the text." .



################ letters ###############

:letters		a rdf:Bag ;
					jena:bag-sort "55" ;
					rdf:member :document-components ;
					dcterms:title "Letters" .

:letter			a rdf:Property ;
					rdf:member :letters ;
					rdfs:label "letter" ;
					rdfs:comment "An instance of a formal, informal or business letter." .

:sender		a rdf:Property ;
					rdf:member :letters ;
					rdfs:label "sender" ;
					rdfs:comment "The name and address of the sender of a letter." .

:recipient		a rdf:Property ;
					rdf:member :letters ;
					rdfs:label "recipient" ;
					rdfs:comment "The name and address of the recipient of a letter." .

:salutation		a rdf:Property ;
					rdf:member :letters ;
					rdfs:label "salutation" ;
					rdfs:comment "An expression of greeting at the start of a letter." .

:valediction		a rdf:Property ;
					rdf:member :letters ;
					rdfs:label "valediction" ;
					rdfs:comment "An expression of farewell at the end of a letter." .

:signature		a rdf:Property ;
					rdf:member :letters ;
					rdfs:label "signature" ;
					rdfs:comment "The signature(s) of the person(s) who sent a letter." .

:postscript		a rdf:Property ;
					rdf:member :letters ;
					rdfs:label "postscript" ;
					rdfs:comment "An additional paragraph or note occurring after a signature in a letter." .

#### emails ####

:emails			a rdf:Bag ;
					jena:bag-sort "60" ;
					rdf:member :letters ;
					dcterms:title "Emails" .

:email			a rdf:Property ;
					rdf:member :emails ;
					rdfs:label "email" ;
					rdfs:comment "An instance of a email message." .

:to			a rdf:Property ;
					rdf:member :emails ;
					rdfs:label "to" ;
					rdfs:comment "The recipient(s) email address line." .

:from			a rdf:Property ;
					rdf:member :emails ;
					rdfs:label "from" ;
					rdfs:comment "The sender's email address line." .

:cc			a rdf:Property ;
					rdf:member :emails ;
					rdfs:label "cc" ;
					rdfs:comment "The carbon copy email address line." .

:bcc			a rdf:Property ;
					rdf:member :emails ;
					rdfs:label "bcc" ;
					rdfs:comment "The blind carbon copy email address line." .

:subject		a rdf:Property ;
					rdf:member :emails ;
					rdfs:label "subject" ;
					rdfs:comment "The email subject line." .



################ lists #################

:collections		a rdf:Bag ;
					jena:bag-sort "65" ;
					rdf:member :document-components ;
					dcterms:title "Lists" .

