<?xml version="1.0"?>
<grammar ns="http://www.loc.gov/mods/v3"
    xmlns:mods="http://www.loc.gov/mods/v3"
    xmlns="http://relaxng.org/ns/structure/1.0" 
    xmlns:xsdp="http://www.daisy.org/ns/rng-preprocess/xsd"
    datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">

    <define name="z3986.head.extra" combine="interleave">
      <optional>    
        <ref name="mods"/>
      </optional>
    </define>
    
    <define name="mods" xsdp:replace="mods.xsd">
      <element name="mods">
        <optional>
          <attribute name="version">
            <value type="string">3.3</value>
          </attribute>
        </optional>
        <optional>
          <attribute name="ID">
            <data type="ID"/>
          </attribute>
        </optional>
        <oneOrMore>
          <ref name="modsGroup"/>
        </oneOrMore>
      </element>
    </define>
    
    <define name="mods.xsd">
      <!-- a placeholder for the real 3.3 xsd -->
      <element name="mods:mods">
        <attribute name="version" />
        <attribute name="ID" />
      </element>
    </define>
 
    <define name="anonymous0">
        <mixed>
            <zeroOrMore>
                <ref name="any"/>
            </zeroOrMore>
        </mixed>
    </define>
    
    <define name="any">
        <choice>
            <element>
                <anyName>
                    <except>
                        <name>modsCollection</name>
                        <name>mods</name>
                        <!-- TODO fix -->
                        <nsName ns="http://www.w3.org/2002/xforms/"/>
                        <nsName ns="http://www.daisy.org/z3986/2010/"/>
                        <nsName ns="http://www.w3.org/2001/10/synthesis"/>
                        <nsName ns="http://www.loc.gov/mods/v3"/>
                        <nsName ns="http://www.w3.org/1998/Math/MathML"/>
                        <nsName ns="http://www.w3.org/2005/11/its"/>
                        <nsName ns="http://www.daisy.org/z3986/2010/auth/features/select/#"/>
                    </except>
                </anyName>
                <mixed>
                    <zeroOrMore>
                        <choice>
                            <attribute>
                                <anyName/>                                
                            </attribute>
                            <ref name="any"/>
                        </choice>
                    </zeroOrMore>
                </mixed>
            </element>
            <ref name="mods"/>
            <ref name="modsCollection"/>
        </choice>
    </define>
    
    <define name="anonymous2">
        <ref name="simpleLink"/>
        <ref name="language"/>
        <ref name="anonymous4"/>
    </define>
    <define name="anonymous1">
        <optional>
            <attribute name="encoding">
                <choice>
                    <value type="string">marc</value>
                    <value type="string">w3cdtf</value>
                    <value type="string">iso8601</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="qualifier">
                <choice>
                    <value type="string">questionable</value>
                    <value type="string">approximate</value>
                    <value type="string">inferred</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="point">
                <choice>
                    <value type="string">start</value>
                    <value type="string">end</value>
                </choice>
            </attribute>
        </optional>
        <data type="string"/>
    </define>
    <define name="titleInfoType">
        <group>
            <group>
                <ref name="idAuthorityXlinkLanguage"/>
                <optional>
                    <attribute name="displayLabel">
                        <data type="string"/>
                    </attribute>
                </optional>
                <zeroOrMore>
                    <choice>
                        <choice>
                            <notAllowed/>
                            <element name="title">
                                <ref name="string"/>
                            </element>
                        </choice>
                        <choice>
                            <notAllowed/>
                            <element name="subTitle">
                                <ref name="string"/>
                            </element>
                        </choice>
                        <choice>
                            <notAllowed/>
                            <element name="partNumber">
                                <ref name="string"/>
                            </element>
                        </choice>
                        <choice>
                            <notAllowed/>
                            <element name="partName">
                                <ref name="string"/>
                            </element>
                        </choice>
                        <choice>
                            <notAllowed/>
                            <element name="nonSort">
                                <ref name="string"/>
                            </element>
                        </choice>
                    </choice>
                </zeroOrMore>
            </group>
            <optional>
                <attribute name="type">
                    <choice>
                        <value type="string">translated</value>
                        <value type="string">uniform</value>
                        <value type="string">alternative</value>
                        <value type="string">abbreviated</value>
                    </choice>
                </attribute>
            </optional>
        </group>
        <empty/>
    </define>
    <define name="stringPlusAuthority">
        <ref name="anonymous7"/>
        <empty/>
    </define>
    <define name="modsCollection">
        <choice>
            <notAllowed/>
            <element name="modsCollection">
                <oneOrMore>
                    <ref name="mods"/>
                </oneOrMore>
            </element>
        </choice>
    </define>
    <define name="language">
        <optional>
            <attribute name="transliteration">
                <data type="string"/>
            </attribute>
        </optional>
        <optional>
            <attribute name="script">
                <data type="string"/>
            </attribute>
        </optional>
        <optional>
            <attribute>
                <name ns="http://www.w3.org/XML/1998/namespace">lang</name>
                <choice>
                    <data type="language"/>
                    <value type="string"></value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="lang">
                <data type="string"/>
            </attribute>
        </optional>
    </define>
    <define name="anonymous3">
        <optional>
            <attribute name="type">
                <data type="string"/>
            </attribute>
        </optional>
        <ref name="anonymous5"/>
    </define>
    <define name="languageType">
        <group>
            <optional>
                <attribute name="objectPart">
                    <data type="string"/>
                </attribute>
            </optional>
            <oneOrMore>
                <choice>
                    <notAllowed/>
                    <element name="languageTerm">
                        <optional>
                            <attribute name="authority">
                                <choice>
                                    <value type="string">iso639-3</value>
                                    <value type="string">iso639-2b</value>
                                    <value type="string">rfc3066</value>
                                    <value type="string">rfc4646</value>
                                </choice>
                            </attribute>
                        </optional>
                        <optional>
                            <attribute name="type">
                                <choice>
                                    <value type="string">text</value>
                                    <value type="string">code</value>
                                </choice>
                            </attribute>
                        </optional>
                        <data type="string"/>
                    </element>
                </choice>
            </oneOrMore>
        </group>
        <empty/>
    </define>
    <define name="modsGroup">
        <choice>
            <choice>
                <notAllowed/>
                <element name="titleInfo">
